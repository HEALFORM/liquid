    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "codecov": "^3.8.3",
    "framer-motion": "^4.1.17",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "patch-package": "^6.4.7",
    "polished": "^4.2.2",
