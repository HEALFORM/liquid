    "color-hash-ts": "^0.0.7",
    "deepmerge": "^4.2.2",
    "eslint": "7.32.0",
    "font-color-contrast": "^9.0.2",
    "framer-motion": "^4.1.17",
    "lint-staged": "^11.2.6",
